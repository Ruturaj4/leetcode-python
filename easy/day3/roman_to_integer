class Solution:
    def romanToInt(self, s: 'str') -> 'int':
        dic = {"I":1, "V":5, "X":10, "L":50, "C":100, "D":500, "M":1000,}
        num = 0
        if "IV" in s or "IX" in s:
            num -= 2
        if "XL" in s or "XC" in s:
            num -= 20
        if "CM" in s or "CD" in s:
            num -= 200
        for l in range(len(s)):
            num += dic[s[l]]
        return num
